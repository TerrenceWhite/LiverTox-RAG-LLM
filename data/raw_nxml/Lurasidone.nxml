<name name-style="western"><surname>Zeiss</surname><given-names>R</given-names></name>
<name name-style="western"><surname>Hafner</surname><given-names>S</given-names></name>
<name name-style="western"><surname>Sch&#x000f6;nfeldt-Lecuona</surname><given-names>C</given-names></name>
